Duplication:
  max_calls: 3

UncommunicativeVariableName:
  accept:
    - "_"
    - "e"
    - "k"
    - "v"
    - "r"

LongMethod:
  max_statements: 5
  exclude:
    - deploy
    - run$
    - run_command$

UtilityFunction:
  exclude:
    - Git
    - Time
    - release_name
    - shell_say
    - convert_string_keys_to_symbols

DataClump:
  exclude:
    - Log

FeatureEnvy:
  exclude:
    - Git
    - convert_string_keys_to_symbols